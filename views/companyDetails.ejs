<!DOCTYPE html>
<html lang="en">
<head>
  
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="css/font-awesome.min.css" rel="stylesheet">
  <link href="css/bootstrap.min.css" rel="stylesheet">
  <link href="css/templatemo-style.css" rel="stylesheet">
  <link rel="stylesheet" href="css/style.css">
  <title>Details</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="css/style.css">
  

           
</head>
<body>
  
  <div class="col-1">
    <div class="panel panel-default templatemo-content-widget white-bg no-padding templatemo-overflow-hidden">
      
      <div class="panel-heading templatemo-position-relative">
        <h2 class="text-uppercase">Student Details</h2>
      </div>
      <div class="table-priya">
        <table class="table table-striped table-tej">
          <div class="table-data">
           
               
              <%
              if(compdetailList.length!=0){
              var i=1;
              compdetailList.forEach(cCompanyDetails => {
              %>
                    <tr><th>Company Name</th> <td><%=cCompanyDetails.compny %></td></tr>
                    <tr><th>Date</th><td><%=cCompanyDetails.date %></td></tr>
                    <tr><th>Campus/Pool</th><td><%=cCompanyDetails.campool %></td></tr>
                    <tr><th>Pool Campus Venue</th> <td><%=cCompanyDetails.pcv %></td></tr>
                    <tr><th>Other Details</th><td><%=cCompanyDetails.odetails %></td></tr>
                    
                
                <%  i++; }) %>
                <% } else{ %>
                    <tc>
                        <td colspan="2">No Data Found</td>
    
                    </tc>
                <% } %>

                  </table>
                 
                  </div>
                  </div>
                  </div>
            </div>
            <div  class="edit"> <a href="./detail.hbs">Edit</a></div>
</body>
</html>



